{
  "name": "flowforge",
  "version": "0.1.0-alpha.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:canvas": "vite --open /test/canvas"
  },
  "dependencies": {
    "@flowforge/canvas": "workspace:*",
    "@flowforge/state": "workspace:*",
    "@flowforge/types": "workspace:*",
    "@node-core/schema": "workspace:*",
    "@node-core/registry": "workspace:*",
    "@node-core/engine": "workspace:*",
    "@node-core/render": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tauri-apps/api": "^1.5.3",
    "@tauri-apps/cli": "^1.5.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "happy-dom": "^20.5.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^4.0.18"
  }
}
